{
    "@id": "http://dylanvanassche.be/cases/transform-mysql-to-ntriples",
    "name": "transform-mysql-to-ntriples",
    "description": "Transform MySQL database into N-Triples with RMLMapper",
    "steps": [
        {
            "@id": "http://dylanvanassche.be/cases/transform-mysql-to-ntriples#step1",
            "name": "Init MySQL RDB",
            "resource": "MySQL",
            "command": "load",
            "parameters": {
                "csv_file": "student.csv",
                "name": "student"
            }
        },
        {
            "@id": "http://dylanvanassche.be/cases/transform-mysql-to-ntriples#step2",
            "name": "Transform",
            "resource": "RMLMapper",
            "command": "execute_mapping",
            "parameters": {
                "mapping_file": "/data/shared/mapping.rml.ttl",
                "output_file": "/data/shared/out.nt",
                "serialization": "nquads",
                "rdb_host": "MySQL",
                "rdb_port": "3306",
                "rdb_username": "root",
                "rdb_password": "root",
                "rdb_type": "MySQL",
                "rdb_name": "db"
            }
        }
    ]
}
