{
    "@id": "http://dylanvanassche.be/cases/transform-csv-to-ntriples",
    "name": "transform-csv-to-ntriples",
    "description": "Transform student.csv into N-Triples with RMLMapper",
    "steps": [
        {
            "@id": "http://dylanvanassche.be/cases/transform-csv-to-ntriples#step1",
            "name": "Transform",
            "resource": "RMLMapper",
            "command": "execute_mapping",
            "parameters": {
                "mapping_file": "/data/shared/mapping.rml.ttl",
                "output_file": "/data/shared/out.nt",
                "serialization": "nquads"
            }
        },
        {
            "@id": "http://dylanvanassche.be/cases/transform-csv-to-ntriples#step2",
            "name": "Load Virtuoso",
            "resource": "Virtuoso",
            "command": "load",
            "parameters": {
                "rdf_file": "/data/shared/out.nt"
            }
        },
        {
            "@id": "http://dylanvanassche.be/cases/transform-csv-to-ntriples#step3",
            "name": "Execute SPO SPARQL query",
            "resource": "Query",
            "command": "execute_and_save",
            "parameters": {
                "query": "PREFIX foaf: <http://xmlns.com/foaf/0.1/> CONSTRUCT WHERE { ?s foaf:name ?o1 . }",
                "sparql_endpoint": "http://localhost:8890/sparql",
                "results_file_name": "results.nt"
            }
        }
    ]
}
