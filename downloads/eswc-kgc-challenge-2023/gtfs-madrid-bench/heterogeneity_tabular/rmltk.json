{
    "@id": "http://example.com/cases/rmltk/Virtuoso/heterogeneity_tabular",
    "name": "rmltk: Virtuoso: MySQL RDB: heterogeneity tabular",
    "description": "Transform MySQL database into N-Triples with rmltk and Virtuoso",
    "steps": [
        {
          "@id": "http://example.com/cases/rmltk/Virtuoso/heterogeneity_files#step00",
          "name": "Fix invalid RML Mapping",
          "resource": "Rpt",
          "command": "execute_capture",
          "parameters": {
            "command": "integrate",
            "arguments": ["mapping.rml.ttl",
			  "prefix rr: <http://www.w3.org/ns/r2rml#> DELETE {?s rr:constant ?c ; rr:termType rr:IRI} INSERT {?s rr:constant ?uri} WHERE {?s rr:constant ?c ; rr:termType rr:IRI filter(isliteral(?c)) BIND(URI(?c) AS ?uri) }",
			  "PREFIX rml: <http://semweb.mmlab.be/ns/rml#> PREFIX rr:  <http://www.w3.org/ns/r2rml#> delete { ?map rr:logicalTable ?src . ?src rr:tableName ?name . } insert { ?map rml:logicalSource ?src . ?src rml:source ?nameCsv ; rml:referenceFormulation  <http://semweb.mmlab.be/ns/ql#CSV> . } where { ?map rr:logicalTable ?src . ?src rr:tableName ?name . bind(concat(ucase(?name),'.csv') as ?nameCsv) }",
			  "spo.rq"],
            "output": "mapping.rml.fixed.ttl"
          }
        },{
          "@id": "http://example.com/cases/rmltk/Virtuoso/heterogeneity_files#step01",
          "name": "Transform mapping into Sparql Query",
          "resource": "Rpt",
          "command": "execute_capture",
          "parameters": {
            "command": "rmltk rml to sparql",
            "arguments": ["mapping.rml.fixed.ttl"],
            "output": "mapping.rml.rq"
          }
        },{
          "@id": "http://example.com/cases/rmltk/Virtuoso/heterogeneity_files#step02",
          "name": "Optimise Sparql Query",
          "resource": "Rpt",
          "command": "execute_capture",
          "parameters": {
            "command": "rmltk optimize workload",
            "arguments": ["mapping.rml.rq", "--no-order"],
            "output": "mapping.rml.optimised.rq"
          }
        },{
          "@id": "http://example.com/cases/rmltk/Virtuoso/heterogeneity_files#step03",
          "name": "Transform data into RDF",
          "resource": "Rpt",
          "command": "execute",
          "parameters": {
            "command": "sansa query",
            "arguments": ["mapping.rml.optimised.rq", "--out-file=out.nt", "--out-overwrite"]
          }
        },

        {
            "@id": "http://example.com/cases/rmltk/Virtuoso/heterogeneity_tabular#step3",
            "name": "Load RDF",
            "resource": "Virtuoso",
            "command": "load",
            "parameters": {
                "rdf_file": "out.nt"
            }
        },
        {
            "@id": "http://example.com/cases/rmltk/Virtuoso/heterogeneity_tabular#step4",
            "name": "Execute SPARQL query 1",
            "resource": "Query",
            "command": "execute_from_file_and_save",
            "parameters": {
                "query_file": "q1.rq",
                "sparql_endpoint": "http://localhost:8890/sparql",
                "results_file": "results1.nt"
            }
        },
